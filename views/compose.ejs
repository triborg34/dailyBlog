<%- include('./partials/header'); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="titletext" class="form-label">Title</label>
    <input type="text" id="titletext" class="form-control" name="title" />
    </div>
    
    <div class="form-group">
    <label for="bodytext" class="form-label">Text</label>
    <textarea name="bodyText" id="bodytext" class="form-control" rows="10"></textarea>
</div>

    <button type="submit" class="btn btn-primary" name="btn">Publish</button>
  
</form>

<%- include('./partials/footer'); -%>
